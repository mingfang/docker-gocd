<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.thoughtworks.cruise.server.domain.oauth" default-access="field" default-lazy="false">
    <class name="GadgetOauthAccessToken" table="gadgetOauthAccessTokens" >
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="userId"/>
        <property name="accessToken" />
        <property name="refreshToken"/>
        <property name="expiresIn"/>
        <property name="gadgetsOauthClientId"/>
    </class>
</hibernate-mapping>