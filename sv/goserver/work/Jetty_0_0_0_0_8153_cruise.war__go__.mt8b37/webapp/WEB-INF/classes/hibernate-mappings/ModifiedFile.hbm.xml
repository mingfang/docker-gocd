<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.thoughtworks.cruise.domain.materials" default-access="field" default-lazy="false">
    <class name="ModifiedFile" table="modifiedFiles" >
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="hibernateAction" column="action" access="property"/>
        <property name="fileName"/>
        <property name="folderName"/>
    </class>
</hibernate-mapping>