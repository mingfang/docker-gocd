<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" default-lazy="false">
    <class name="com.thoughtworks.cruise.server.domain.user.PipelineSelections" table="pipelineSelections" >
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="unselectedPipelines" access="property"/>
        <property name="userId" access="field"/>
        <property name="lastUpdate"/>
    </class>
</hibernate-mapping>