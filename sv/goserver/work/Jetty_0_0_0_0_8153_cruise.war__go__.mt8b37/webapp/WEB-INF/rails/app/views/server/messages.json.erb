{
    "cruise_message_counts" : {"html" : "<% if !@current_server_health_states.isRealSuccess() %>\n    <a onclick=\"Modalbox.show($('cruise_message_body'), {title: 'Error and warning messages', overlayClose:false})\">\n        <span class=\"messages\">\n            <% if @current_server_health_states.warningCount() > 0  && @current_server_health_states.errorCount() > 0 %>\n                <span class=\"error_count\"><%= json_escape((l.errorCount(@current_server_health_states.errorCount()))) -%></span> &\n                <span class=\"warning_count\"><%= json_escape((l.warningCount(@current_server_health_states.warningCount()))) -%></span>\n            <% else %>\n                <% if @current_server_health_states.errorCount() > 0 %>\n                    <span class=\"error_count\"><%= json_escape((l.errorCount(@current_server_health_states.errorCount()))) -%></span>\n                <% end %>\n                <% if @current_server_health_states.warningCount() > 0 %>\n                    <span class=\"warning_count\"><%= json_escape((l.warningCount(@current_server_health_states.warningCount()))) -%></span>\n                <% end %>\n            <% end %>\n        </span>\n    </a>\n<% end %>"},
    "cruise_message_body" : {"html" : "<% if !@current_server_health_states.isRealSuccess() %>\n        <% @current_server_health_states.each do |state_in_messages_body| %>\n            <% if !state_in_messages_body.isRealSuccess() %>\n                <div class=\"<%= json_escape((state_in_messages_body.getLogLevel().name().downcase)) -%>\">\n                    <div class=\"message\"><%= json_escape((state_in_messages_body.getMessageWithTimestamp())) -%></div>\n                    <div class=\"description\"><%= json_escape((state_in_messages_body.getDescription())) -%></div>\n                </div>\n            <% end %>\n        <% end %>\n<% end %>"}
}