## layout level variable
#set ($title = "Server Details - Go")
#set($extra_css_list = ['admin-tab', 'sub_tab'])
## page level variable
#set($_hide_collapse-sidebar-button = true)
#set($_page_title="Server Details")
#parse("shared/_header.vm")

<script type="text/javascript">var noErrorLog = true;</script>
<div id="yui-main">
    <div class="yui-b" id="server-page">
        #if(${success_message})
            #parse('shared/_flash_success.vm')
        #end
        #if(${errorMessage})
            #parse('shared/_flash_error.vm')
        #end
        <!-- breadcrumbs -->
        #set($current_page="no-crumbs")
        #parse("shared/_breadcrumbs.vm")
        <!-- /breadcrumbs -->

        #parse("shared/_page_intro_top.vm")
        <p>This view shows basic configuration information for Go Server.
            Administrators can also manage the license information from here.
            <a href="$req.getContextPath()/help/server_details_page.html" title="Learn more from help documentation" target="_blank" class="more">more...</a>
        </p>
        #parse("shared/_page_intro_bottom.vm")


        <div id="build-status-panel" class="bd-container rounded-corner-for-pipeline">
            #if($userHasAdministratorRights)

                <div id="content-of-license">            
                    <h1>License</h1>

                        <form action="$req.getContextPath()/about" name="license_form"  method="POST">
                            <label for="user">User:</label> <input id="license_for_user" type="text" name="license_for_user" value="${user}" style="float: none;"/>
                            <label for="license">License key:</label>
                            <textarea name="license" id="license" class="width-full" rows="5" cols="20">${license}</textarea>
                            <button type="submit" id="save" class="submit" value="Save license"><span>Save License</span></button>
                        </form>

                </div>

            #end

            <div id="content-of-about">
                <h1>Server Info</h1>                

                <table id="server-details-table">
                    <tbody>
                        <tr class="odd">
                            <td>Go Server Version:</td>
                            <td class="value_column">#include("admin/admin_version.txt.vm")</td>
                        </tr>
                        <tr class="even">
                            <td>JVM version:</td>
                            <td class="value_column">$!{jvm_version}</td>
                        </tr>
                        <tr class="odd">
                            <td>OS Information:</td>
                            <td class="value_column">$!{os_info}</td>
                        </tr>
                        <tr class="even">
                            <td>Usable space in artifacts repository:</td>
                            <td class="value_column">$!{available_space} Mb</td>
                        </tr>
                        <tr class="odd">
                            <td>Database schema version: </td>
                            <td class="value_column">$!{schema_version}</td>
                        </tr>
                        <tr class="even">
                            <td>License expiry date:</td>
                            <td class="value_column">$!{expiry_date}</td>
                        </tr>
                        <tr class="odd">
                            <td>Number of remote agents Go is licensed for: </td>
                            <td class="value_column">$!{max_agents}</td>
                        </tr>
                        <tr class="even">
                            <td>Number of users Go is licensed for: </td>
                            <td class="value_column">$!{max_users}</td>
                        </tr>
                        <tr class="odd">
                            <td>Go edition: </td>
                            <td class="value_column">$!{edition.displayName}</td>
                        </tr>
                    </tbody>
                </table>
            </div>



